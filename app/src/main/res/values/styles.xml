<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Button">
        <item name="android:background">@drawable/background_button</item>
        <item name="fontFamily">@font/poppins_medium</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="textAllCaps">false</item>
    </style>

    <style name="ButtonSecondary">
        <item name="android:background">@drawable/background_button_secondary</item>
        <item name="fontFamily">@font/poppins_medium</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/dark_purple</item>
        <item name="textAllCaps">false</item>
    </style>

    <style name="tvdetailresult">
        <item name="fontFamily">@font/poppins_semibold</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/dark_purple</item>
    </style>
    <style name="tvdetailresult2">
        <item name="fontFamily">@font/poppins_regular</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/dark_purple</item>
    </style>

    <style name="slider" parent="Theme.Material3.DayNight.NoActionBar">
    </style>

    <style name="theme" parent="slider"/>
</resources>